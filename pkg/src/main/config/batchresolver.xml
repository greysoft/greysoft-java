<naf>
	<dnsresolver recursive="n" exitdump="Y" class="com.grey.naf.dns.embedded.EmbeddedResolver">
		<udpsockets>10</udpsockets>
	</dnsresolver>
	<dispatchers>
		<dispatcher name="dnsbatch" dns="Y" zero_naflets="N">
			<naflets>
				<naflet name="batchdns" class="com.grey.naf.dns.batchresolver.BatchResolver">
					<dnstype>MX</dnstype>
					<infile>infile.txt</infile>
					<outfile>outfile.txt</outfile>
					<maxpending>100</maxpending>
				</naflet>
			</naflets>
		</dispatcher>
	</dispatchers>
</naf>
