<naf>
	<dnsresolver recursive="n" exitdump="Y" class="com.grey.naf.dns.resolver.embedded.EmbeddedResolver">
		<udpsockets>10</udpsockets>
	</dnsresolver>
	<dispatchers>
		<dispatcher name="dnsbatch" dns="Y" zero_naflets="N">
			<naflets>
				<naflet name="batchdns" class="com.grey.dnsbatchresolver.BatchResolver">
					<dnstype>MX</dnstype>
					<infile>${HOME}/infile.txt</infile>
					<outfile>${HOME}/outfile.txt</outfile>
					<maxpending>100</maxpending>
				</naflet>
			</naflets>
		</dispatcher>
	</dispatchers>
</naf>
